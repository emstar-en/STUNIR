{
  "module": "nested_4_levels",
  "description": "Test 4-level nesting: while inside if inside if inside if",
  "functions": [
    {
      "name": "test_nested_4",
      "returns": "i32",
      "params": [{"name": "x", "type": "i32"}],
      "body": [
        {
          "type": "if",
          "condition": "x > 0",
          "then": [
            {
              "type": "if",
              "condition": "x > 10",
              "then": [
                {
                  "type": "if",
                  "condition": "x > 20",
                  "then": [
                    {
                      "type": "while",
                      "condition": "x > 0",
                      "body": [
                        {"type": "assign", "target": "x", "value": "x - 1"}
                      ]
                    },
                    {"type": "return", "value": "0"}
                  ]
                }
              ]
            }
          ]
        },
        {"type": "return", "value": "x"}
      ]
    }
  ]
}
