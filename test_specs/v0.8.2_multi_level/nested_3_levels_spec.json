{
  "module": "nested_3_levels",
  "description": "Test 3-level nesting: if inside if inside if",
  "functions": [
    {
      "name": "test_nested_3",
      "returns": "i32",
      "params": [{"name": "x", "type": "i32"}],
      "body": [
        {
          "type": "if",
          "condition": "x > 0",
          "then": [
            {
              "type": "if",
              "condition": "x > 10",
              "then": [
                {
                  "type": "if",
                  "condition": "x > 20",
                  "then": [{"type": "return", "value": "200"}],
                  "else": [{"type": "return", "value": "100"}]
                }
              ]
            },
            {"type": "return", "value": "10"}
          ]
        }
      ]
    }
  ]
}
