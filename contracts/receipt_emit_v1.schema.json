{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "STUNIR Emit Receipt v1",
  "type": "object",
  "required": [
    "schema",
    "target",
    "status",
    "emit_epoch",
    "receipt_core_id_sha256"
  ],
  "properties": {
    "schema": {
      "const": "stunir.receipt.emit.v1"
    },
    "target": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "ok",
        "error"
      ]
    },
    "emit_epoch": {
      "type": "integer"
    },
    "ir_sha256": {
      "type": "string",
      "pattern": "^[0-9a-f]{64}$"
    },
    "template_pack_sha256": {
      "type": "string",
      "pattern": "^[0-9a-f]{64}$"
    },
    "emitter": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": true,
      "properties": {
        "id": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "sha256": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[0-9a-f]{64}$"
        },
        "version_text_sha256": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[0-9a-f]{64}$"
        }
      }
    },
    "inputs": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": true
    },
    "outputs": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "object",
        "required": [
          "path",
          "sha256"
        ],
        "properties": {
          "path": {
            "type": "string"
          },
          "sha256": {
            "type": "string",
            "pattern": "^[0-9a-f]{64}$"
          }
        },
        "additionalProperties": true
      }
    },
    "argv": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "platform": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": true
    },
    "errors": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "receipt_core_id_sha256": {
      "type": "string",
      "pattern": "^[0-9a-f]{64}$"
    }
  },
  "additionalProperties": true
}
