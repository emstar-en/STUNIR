{
  "schema": "stunir_ir_v1",
  "ir_version": "v1",
  "module_name": "switch_fallthrough_test",
  "docstring": "Test switch/case with fall-through behavior - v0.9.0",
  "types": [],
  "functions": [
    {
      "name": "handle_command",
      "args": [
        {
          "name": "cmd",
          "type": "i32"
        }
      ],
      "return_type": "i32",
      "steps": [
        {
          "op": "assign",
          "target": "status",
          "value": "0"
        },
        {
          "op": "switch",
          "expr": "cmd",
          "cases": [
            {
              "value": 1,
              "body": [
                {
                  "op": "assign",
                  "target": "status",
                  "value": "status + 1"
                }
              ]
            },
            {
              "value": 2,
              "body": [
                {
                  "op": "assign",
                  "target": "status",
                  "value": "status + 10"
                },
                {
                  "op": "break"
                }
              ]
            },
            {
              "value": 3,
              "body": [
                {
                  "op": "assign",
                  "target": "status",
                  "value": "100"
                },
                {
                  "op": "break"
                }
              ]
            }
          ],
          "default": [
            {
              "op": "assign",
              "target": "status",
              "value": "-1"
            }
          ]
        },
        {
          "op": "return",
          "value": "status"
        }
      ]
    }
  ]
}