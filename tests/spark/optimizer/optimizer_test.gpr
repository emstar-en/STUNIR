-- Phase 3b: Optimizer Tests

with "../../../tools/spark/stunir_tools.gpr";

project Optimizer_Test is

   for Source_Dirs use (".", "../../../tools/spark");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";

   for Main use ("test_optimizer.adb");

   package Compiler is
      for Default_Switches ("Ada") use
        ("-gnatwa",
         "-gnat2012",
         "-g",
         "-O2");
   end Compiler;

   package Builder is
      for Default_Switches ("Ada") use ("-g");
   end Builder;

   package Binder is
      for Default_Switches ("Ada") use ("-Es");
   end Binder;

end Optimizer_Test;