project Manifest_Generate is
   for Source_Dirs use ("src/core", "src/verification");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("manifest_generate.adb");
   
   package Compiler is
      for Default_Switches ("Ada") use ("-gnatwa", "-g");
   end Compiler;
end Manifest_Generate;