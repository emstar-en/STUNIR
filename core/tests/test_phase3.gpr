--  STUNIR Phase 3 Test Project
--  Test Infrastructure Tests

with "../stunir_phase3.gpr";
with "../stunir_core.gpr";

project Test_Phase3 is

   for Source_Dirs use (".");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("test_phase3.adb");

   package Compiler is
      for Default_Switches ("Ada") use (
         "-gnat2012",
         "-gnata",
         "-gnatwa",
         "-g"
      );
   end Compiler;

   package Binder is
      for Default_Switches ("Ada") use ("-Es");
   end Binder;

end Test_Phase3;
